<h1>Lista de usuarios</h1>
<table>
	<thead>
		<tr>
			<th>#</th>
			<th>Usuario</th>
			<th>Email</th>
			<th>dni</th>
			<th>perfil</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @user.each do |u| %>
			<tr>
				<td><%= u.id %></td>
				<td><%= u.surname + ', ' + u.name %></td>
				<td><%= u.email %></td>
				<td><%= u.dni %></td>
				<td><%= u.user_type %></td>
				<td><%= link_to 'Ver', user_path(u.id) %></td>
				<td><%= link_to 'Editar', edit_user_path(u.id) %></td>
				<td>borrar</td>
			</tr>	
		<% end %>
	</tbody>
</table>